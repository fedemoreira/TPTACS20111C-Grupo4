<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	 xmlns="http://java.sun.com/xml/ns/javaee"
	 xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	 xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
			     http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <servlet>
    <servlet-name>ServletMercadoLibre</servlet-name>
    <servlet-class>grupo4.servlets.ServletMercadoLibre</servlet-class>
  </servlet> <servlet>
    <servlet-name>ServletWishlist</servlet-name>
    <servlet-class>grupo4.servlets.ServletWishlist</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>ServletMercadoLibre</servlet-name>  
    <url-pattern>/ServletMercadoLibre</url-pattern>
  </servlet-mapping>  <servlet-mapping>
    <servlet-name>ServletWishlist</servlet-name>  
    <url-pattern>/ServletWishlist</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>
