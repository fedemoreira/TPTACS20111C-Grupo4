<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>MercadoLibreServlet</display-name>

    <servlet>
        <servlet-name>ServletMercadoLibre</servlet-name>
	<servlet-class>main.java.grupo4.servlets.ServletMercadoLibre</servlet-class>
    </servlet>


    <servlet>
        <servlet-name>Index</servlet-name>
	<jsp-file>/src/main/webapp/index.jsp</jsp-file>
    </servlet>


    <servlet-mapping>
	    <servlet-name>ServletMercadoLibre</servlet-name>
	    <url-pattern>/ServletMercadoLibre</url-pattern>
    </servlet-mapping>


    <servlet-mapping>
	    <servlet-name>Index</servlet-name>
	    <url-pattern>/</url-pattern>
    </servlet-mapping>
</web-app>
